@model Domain.Application.Dto.Configuration.SSOSettingViewModel

	<div class="box-body">
		<div class="form-group">
			<div class="col-sm-10 icheck-primary d-inline">
				<input type="checkbox" asp-for="EnableGoogleAuth0" class="form-control" />
				<label asp-for="EnableGoogleAuth0" class="col-sm-2 control-label"></label>
			</div>
		</div>
		<div class="form-group">
			<label asp-for="GoogleClientId" class="col-sm-2 control-label"></label>
			<div class="col-sm-10">
				<input asp-for="GoogleClientId" class="form-control" />
				<span asp-validation-for="GoogleClientId" class="text-danger"></span>
			</div>
		</div>
		<div class="form-group">
			<label asp-for="GoogleClientSecret" class="col-sm-2 control-label"></label>
			<div class="col-sm-10">
				<input asp-for="GoogleClientSecret" class="form-control" />
				<span asp-validation-for="GoogleClientSecret" class="text-danger"></span>
			</div>
		</div>
	</div>