<script type="text/javascript" defer="">
    $(document).ready(function() {
        function sendPostRequest(url, postData, callback) {
            $.ajax(url,
                {
                    type: "POST",
                    dataType: "text",
                    success: callback,
                    error: function(xhr, ajaxOptions, thrownError) {
                        //alert('eerroorr');
                    }
                });
        }

        function validateEmail(email) {
            var expr =
                /^([\w-\.]+)@@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
            if (!expr.test(email)) {
                return false;
            }
            return true;
        }

        $(".order-submit").on('click',
            function(e) {
                e.preventDefault();
                $.ajax({
                    type: 'POST',
                    url: "/Order/CheckLogin/",
                    dataType: 'json',
                    success: function(result) {
                        if (result.success === true) {
                            alert("Chúc quý khách mua hàng vui vẻ !!!!");
                        } else {
                            alert(result.message);
                        }
                    },
                    error: function(xhr, ajaxOptions, error) {
                        alert('Đã xảy ra lỗi khi thực hiện yêu cầu của bạn. Vui lòng kiểm tra và thử lại!');
                    },
                    complete: function(result) {
                        location.href = '@Url.Content("~/Order/HomeOrder")';
                    }
                });
            });

        function compareLargeStrings(a, b) {
            if (a.length !== b.length) {
                return false;
            }
            return a.localeCompare(b) === 0;
        }

        $('#crBtn').click(function(e) {
            e.preventDefault();
            var name = $('#crName').val();
            var email = $('#crEmail').val();
            var password = $('#crPassword').val();
            var repassword = $('#crRePassword').val();
            var data = {};
            data.fullName = name;
            data.email = email;
            data.password = password;

            if (name === '' || password === '' || !compareLargeStrings(password.trim(), repassword.trim())) {
                e.preventDefault();
                Swal.fire('Kiểm tra lại dữ liệu nhập vào', "", "error");

                if (!validateEmail(email)) {
                    e.preventDefault();
                    Swal.fire('Email không hợp lệ', "", "error");
                }
            } else {
                e.preventDefault();
                $.blockUI({ message: '<h1><img src="~/throbber.gif"/> Just a moment...</h1>' });

                $.ajax({
                    type: 'Post',
                    contentType: "application/json; charset=utf-8",
                    data: JSON.stringify(data),
                    url: '@Url.Action("AddUser", "Auth")',

                    success: function(data) {
                        if (data.includes("thành công")) {
                            $.unblockUI();
                            Swal.fire(
                                data,
                                '',
                                'success'
                            );
                            $('#closeModalBtn').click();
                            location.href = '@Url.Content("~/Home/Index")';
                        } else {
                            e.preventDefault();
                            $.unblockUI();
                            Swal.fire(data.Message, "", "error");
                        }
                    },
                    error: function() {
                        $.unblockUI();
                        Swal.fire('Data sending error ...', "", "error");
                    }
                });

            }
        });
        $('#forgotBtn').on("click",
            function(e) {
                e.preventDefault();
                var bla = $('#forgot-email').val();
                var sData = {
                    email: bla
                };
                $.blockUI({ message: '<h1><img src="~/busy.gif" /> Just a moment...</h1>' });


                $.ajax({
                    url: "/Auth/ForgotPassword",
                    data: sData,
                    type: "GET",
                    dataType: "json",
                    cache: false,
                    success: function(result) {
                        $.unblockUI();
                        Swal.fire(
                            result,
                            '',
                            'success'
                        );
                    },
                    error: function(jqXHR, textStatus) {
                        $.unblockUI();
                        Swal.fire('Data sending error ...', "", "error");
                    }
                });
            });
        $('#loginBtn').click(function(e) {
            var email = $('#loginEmail').val();
            var password = $('#loginPassword').val();
            var user = {
                Email: email,
                Password: password
            }
            $.blockUI({ message: '<h1><img src="~/busy.gif" /> Just a moment...</h1>' });
            e.preventDefault();
            $.ajax({
                type: 'Post',
                traditional: true,
                async: false,
                cache: false,
                url: '/Auth/Login/' + JSON.stringify(user),
                success: function(result) {
                    if (result.includes("thành công")) {
                        $.unblockUI();
                        Swal.fire(
                            result,
                            '',
                            'success'
                        );
                        $('#closeModalBtn').click();
                        location.href = '@Url.Content("~/Home/Index")';
                    } else {
                        $.unblockUI();
                        Swal.fire(result, "", "error");
                    }
                },
                error: function(xhr) {
                    $.unblockUI();
                    console.log(xhr.responseText);
                    Swal.fire('Data sending error ...', "", "error");
                }
            });

        });

        $('#logoutBtn').click(function(e) {
            e.preventDefault();
            $.ajax({
                type: 'GET',
                url: '/Auth/Logout',
                success: function(result) {

                    location.reload();
                    Swal.fire(
                        result,
                        '',
                        'success'
                    );
                }
            });
        });


    });
</script>
