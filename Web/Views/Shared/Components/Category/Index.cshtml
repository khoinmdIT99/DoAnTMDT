@using Web.Component
<li class="full-width menu-dinning with-sub-menu hover">
    <p class="close-menu"></p>
    <a href="#" class="smooth cleafix" title="">
        CATEGORY
        <b class="caret"></b>
    </a>
    <div class="sub-menu">
        <div class="content">
            <div class="row">
                <div class="col-sm-12">
                    <div class="html">
                        <div class="row">
                            @{
                                var dem = 0;
                            }
                            @foreach (var item in (List<HienThiCategory>)ViewBag.HienthiCategories)
                            {
                                <div class="col-md-3" style="margin-top: 5px">
                                    <div class="column">
                                        <div class="static-menu-image">
                                            <img src="~/Test/image/catalog/demo/slider/home1/static-menu-image2.jpg" alt="">
                                        </div>
                                        <a href="#" class="smooth title-submenu" title="">@item.DeMuc</a>
                                        <div>
                                            <ul class="row-list">
                                                @foreach (var i in item.Categoryy)
                                                {
                                                    if (i.Listcategoryphu.Count == 0)
                                                    {
                                                        <li>
                                                            <a href="#" class="smooth" title="">@i.Tencategory</a>
                                                        </li>
                                                    }
                                                    else
                                                    {
                                                        <li>
                                                            <a href="#" class="smooth" title="">
                                                                @i.Tencategory
                                                            </a>
                                                            <ul>
                                                                @foreach (var j in i.Listcategoryphu)
                                                                {
                                                                    <li>
                                                                        <a href="#" class="smooth" title="">
                                                                            @j
                                                                        </a>
                                                                    </li>
                                                                }
                                                            </ul>
                                                        </li>
                                                    }

                                                }
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                                dem++;
                            }
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</li>

